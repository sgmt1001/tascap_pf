<div class="container">
  <div class="row my-3">
    <div class="col-lg-12">
      <h1>project edit</h1>

      <%= form_with model:@project,url:organization_project_path(@project.id),local:true do |f| %>
        <div>
          <%= f.label :name %><br>
          <%= f.text_field :name %>
        </div>
        <div>
          <%= f.label :overview %><br>
          <%= f.text_area :overview %>
        </div>
        <div>
          <%= f.submit class: 'btn btn-success' %>
        </div>
      <% end %>


    </div>
  </div>
</div>